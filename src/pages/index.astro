---
import Layout from "../layouts/Layout.astro";
import { useCloud } from "freestyle-sh";
import { SimpleRepo } from "../cloudstate/simple-repo";
import {RepoLayout} from "../layouts/Repo.tsx";
const simpleRepo = useCloud<typeof SimpleRepo>("simple-repo")

const repoInfo = await simpleRepo.getInfo()
---

<Layout title="Freestyle.sh Project">
  <RepoLayout client:load repoMetadata={repoInfo} >
</Layout>
